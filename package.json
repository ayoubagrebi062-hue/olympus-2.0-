{
  "name": "olympus-2.0",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "validate": "node scripts/validate-code-quality.js",
    "predev": "node scripts/validate-code-quality.js || true",
    "dev": "next dev -p 3001",
    "prebuild": "node scripts/validate-code-quality.js",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --exclude tests/e2e/** --exclude tests/load/**",
    "test:security": "vitest run tests/security/",
    "test:load": "RUN_LOAD_TESTS=true vitest run tests/load/",
    "test:chunk": "npm run type-check && npm run lint",
    "test:full": "npm run type-check && npm run lint && npm run build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:a11y": "playwright test tests/e2e/accessibility.spec.ts",
    "test:all": "npm run test:unit && npm run test:security && npm run test:e2e",
    "check:circular": "madge --circular --extensions ts,tsx src/",
    "check:imports": "tsc --noEmit --traceResolution 2>&1 | grep -i error || echo 'All imports OK'",
    "docs:readme": "tsx scripts/docs/generate-readme.ts",
    "docs:adr": "tsx scripts/docs/generate-adr.ts",
    "docs:api": "tsx scripts/docs/generate-api-docs.ts",
    "docs:all": "tsx scripts/docs/generate-all.ts",
    "qa:index": "tsx scripts/codebase-qa/index-codebase.ts",
    "qa:ask": "tsx scripts/codebase-qa/ask.ts",
    "qa": "tsx scripts/codebase-qa/ask.ts",
    "stress-test": "npx tsx src/lib/agents/stress-test/stress-test.ts",
    "stress-test:watch": "npx tsx --watch src/lib/agents/stress-test/stress-test.ts",
    "stress-test:quick": "npx tsx src/lib/agents/stress-test/quick-test.ts",
    "stress-test:mock": "npx cross-env MOCK_GENERATOR=true npx tsx src/lib/agents/stress-test/stress-test.ts",
    "leonardo:test": "npx tsx src/lib/agents/stress-test/image-integration-test.ts",
    "leonardo:mock": "npx cross-env MOCK_LEONARDO=true npx tsx src/lib/agents/stress-test/image-integration-test.ts",
    "test:images": "npx tsx src/lib/agents/stress-test/image-quick-test.ts",
    "pollinations:test": "npx tsx src/lib/agents/stress-test/image-quick-test.ts",
    "test:conversion": "npx tsx tests/conversion-agents-test.ts",
    "build:verify": "npx tsx scripts/verify-build.ts",
    "build:verify:quick": "npx tsx scripts/verify-build.ts --skip-smoke --timeout=120000",
    "build:verify:verbose": "npx tsx scripts/verify-build.ts --verbose --keep-output",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@codesandbox/sandpack-react": "^2.20.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@monaco-editor/react": "^4.7.0",
    "@qdrant/js-client-rest": "^1.7.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.10",
    "@types/uuid": "^10.0.0",
    "@upstash/qstash": "^2.8.4",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.0",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.27.1",
    "ioredis": "^5.9.1",
    "lucide-react": "^0.562.0",
    "monaco-editor": "^0.55.1",
    "mongodb": "^7.0.0",
    "neo4j-driver": "^6.0.1",
    "next": "^14.2.35",
    "openai": "^6.15.0",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.71.1",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "stripe": "^20.1.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "zustand": "^5.0.10"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ],
    "*.{js,jsx,json,md,css}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.0",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "groq-sdk": "^0.37.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "madge": "^8.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.16",
    "zod": "^3.25.76"
  }
}
